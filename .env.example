# Trackr Health App - Environment Variables
# Copy this file to .env and fill in your values

# Claude API Key (required for AI features)
# Get your API key at https://console.anthropic.com/
EXPO_PUBLIC_CLAUDE_API_KEY=your_claude_api_key_here

# Firebase Configuration (optional - enables Google Sign-In)
# Get these from Firebase Console: https://console.firebase.google.com/
EXPO_PUBLIC_FIREBASE_API_KEY=
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=
EXPO_PUBLIC_FIREBASE_PROJECT_ID=
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
EXPO_PUBLIC_FIREBASE_APP_ID=

# Google Sign-In Client IDs (optional - required for Google Sign-In)
# Get these from Google Cloud Console: https://console.cloud.google.com/apis/credentials
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=
